{
    "name": "adriankristanto.github.io",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
        "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
        "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "cy:test-unit": "cypress run --component",
        "cy:open-unit": "cypress open --component",
        "cy:test-e2e": "cypress run",
        "cy:run-e2e": "start-server-and-test dev http://localhost:5173 cy:test-e2e",
        "cy:open-e2e": "cypress open --e2e",
        "cy:e2e": "start-server-and-test dev http://localhost:5173 cy:open-e2e"
    },
    "dependencies": {
        "framer-motion": "^7.9.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.10.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@vitejs/plugin-react": "^3.0.0",
        "autoprefixer": "^10.4.13",
        "cypress": "^12.1.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
